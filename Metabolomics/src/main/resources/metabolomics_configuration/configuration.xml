<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
    <isatab-configuration table-name="metaboliteprofiling_mztab">
        <measurement term-label="metabolite profiling" term-accession="" source-abbreviation=""/>
        <technology term-label="" term-accession="" source-abbreviation=""/>
        <field header="identifier" data-type="String" is-file-field="false" is-multiple-value="true"
               is-required="true">
            <description>
                A list of “|” separated possible identifiers for these small molecules. e.g. “KEGG:C000017”“CHEBI:12345”
            </description>
            <default-value></default-value>
        </field>
        <field header="chemical_formula" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The chemical formula of the identified compound.</description>
            <default-value></default-value>
        </field>
        <field header="description" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The small molecule's description/name</description>
            <default-value></default-value>
        </field>
        <field header="mass_to_charge" data-type="Double" is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The precursor's experimental mass to charge (m/z).
            </description>
            <default-value></default-value>
        </field>
        <field header="charge" data-type="Double" is-file-field="false"
               is-multiple-value="false" is-required="false">
            <description>The precursor's charge</description>
            <default-value></default-value>
        </field>
        <field header="retention_time" data-type="String" is-file-field="false"
               is-multiple-value="false" is-required="false">
            <description>Small molecule retention time</description>
            <default-value></default-value>
        </field>
        <field header="taxid" data-type="String" is-file-field="false"
               is-multiple-value="false" is-required="false">
            <description>the detector used by the mass spectrometer, use PSI-MS cv</description>
            <default-value></default-value>
        </field>
        <field header="species" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false" is-required="false">
            <description>the detector used by the mass spectrometer, use PSI-MS cv</description>
            <default-value></default-value>
        </field>
    </isatab-configuration>
</isatab-config-file>